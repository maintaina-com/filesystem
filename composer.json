            "Horde\\Filesystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Filesystem\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
